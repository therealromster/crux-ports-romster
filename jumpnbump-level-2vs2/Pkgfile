# Description: 2vs2 level for jumpnbump.
# URL: http://www.jumpbump.mine.nu/
# Maintainer: Danny Rawlins, romster at shortcircuit dot net dot au
# Packager: Danny Rawlins, romster at shortcircuit dot net dot au
# Depends on: jumpnbump

name=jumpnbump-level-2vs2
version=1
release=1
source=(http://www.jumpbump.mine.nu/levels.bz2/2vs2.dat.bz2)

build() {
	bunzip2 2vs2.dat.bz2
	install -D -m644 $SRC/2vs2.dat $PKG/usr/share/jumpnbump/2vs2.dat
}

