# Description: trainin level for jumpnbump.
# URL: http://www.jumpbump.mine.nu/
# Maintainer: Danny Rawlins, romster at shortcircuit dot net dot au
# Packager: Danny Rawlins, romster at shortcircuit dot net dot au
# Depends on: jumpnbump

name=jumpnbump-level-trainin
version=1
release=1
source=(http://www.jumpbump.mine.nu/levels.bz2/trainin.dat.bz2)

build() {
	bunzip2 trainin.dat.bz2
	install -D -m644 $SRC/trainin.dat $PKG/usr/share/jumpnbump/trainin.dat
}

