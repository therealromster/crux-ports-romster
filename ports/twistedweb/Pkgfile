# Description: Python event-driven networking framework.
# URL: http://www.twistedmatrix.com/
# Packager: Danny Rawlins, monster dot romster at gmail dot com
# Maintainer: Danny Rawlins, monster dot romster at gmail dot com
# Depends on: twisted

name=twistedweb
sname=TwistedWeb
version=10.1.0
release=1
source="http://tmrc.mit.edu/mirror/twisted/Web/${version%.*}/$sname-$version.tar.bz2"
up2date_url="http://tmrc.mit.edu/mirror/twisted/Web/"
up2date_branch_filter='[0-9]+.[0-9]+'

build() {
	cd $sname-$version
	python setup.py install --root=$PKG
	find $PKG -name '*.egg-info'  \
	    -o -name topfiles \
	    | xargs rm -rf
}
