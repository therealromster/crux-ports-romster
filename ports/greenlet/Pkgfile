# Packager: Danny Rawlins, monster dot romster at gmail dot com
# Depends on: distribute

name=greenlet
version=0.3.3
release=1
source=(http://pypi.python.org/packages/source/g/$name/$name-$version.zip)

build() {
	cd $name-$version
	python setup.py install --root=$PKG
}
