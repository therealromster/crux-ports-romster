# Description: pyramid level for jumpnbump.
# URL: http://www.jumpbump.mine.nu/
# Maintainer: Danny Rawlins, romster at shortcircuit dot net dot au
# Packager: Danny Rawlins, romster at shortcircuit dot net dot au
# Depends on: jumpnbump

name=jumpnbump-level-pyramid
version=1
release=1
source=(http://www.jumpbump.mine.nu/levels.bz2/pyramid.dat.bz2)

build() {
	bunzip2 pyramid.dat.bz2
	install -D -m644 $SRC/pyramid.dat $PKG/usr/share/jumpnbump/pyramid.dat
}

