# Description: thepit level for jumpnbump.
# URL: http://www.jumpbump.mine.nu/
# Maintainer: Danny Rawlins, romster at shortcircuit dot net dot au
# Packager: Danny Rawlins, romster at shortcircuit dot net dot au
# Depends on: jumpnbump

name=jumpnbump-level-thepit
version=1
release=1
source=(http://www.jumpbump.mine.nu/levels.bz2/thepit.dat.bz2)

build() {
	bunzip2 thepit.dat.bz2
	install -D -m644 $SRC/thepit.dat $PKG/usr/share/jumpnbump/thepit.dat
}

