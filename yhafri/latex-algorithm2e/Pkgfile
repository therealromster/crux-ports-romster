# Depends on: tetex

name=latex-algorithm2e
version=0.9
release=1
source=(algorithm2e.sty)

build () {

	install -D -o root -g root -m 0644 algorithm2e.sty \
        $PKG/usr/share/texmf/tex/latex/algorithm/algorithm2e.sty    
}
